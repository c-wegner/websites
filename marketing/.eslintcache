r\n  md: '825px',\r\n  lg: '992px',\r\n  xl: '1200px',\r\n\r\n  getStringFromSize: function (sz: string): number {\r\n    let temp = sz.substring(0, sz.length - 2);\r\n    return parseInt(temp);\r\n  }\r\n}\r\n\r\nexport const scrollTo = (target: number, speed = 40) => {\r\n  let currentPosition = window.pageYOffset;\r\n\r\n  let steps = 0;\r\n  let thisStep: number;\r\n  let thisTimer: any;\r\n\r\n  const goal = target - 75;\r\n\r\n\r\n  if (currentPosition < goal) {\r\n\r\n    thisTimer = setInterval(\r\n      () => {\r\n        steps += 1;\r\n        thisStep = (steps * speed) + currentPosition;\r\n        window.scrollTo(0, thisStep)\r\n        if (thisStep > goal) {\r\n          clearInterval(thisTimer)\r\n          return;\r\n        }\r\n      }, 1\r\n    )\r\n  } else {\r\n    thisTimer = setInterval(\r\n      ()=>{\r\n        steps +=1;\r\n        thisStep = currentPosition - (steps* speed)\r\n        window.scrollTo(0, thisStep)\r\n        if(thisStep<goal){\r\n          clearInterval(thisTimer)\r\n          return\r\n        }\r\n      }, 1\r\n    )\r\n  }\r\n}","C:\\Users\\Chris Wegner\\Desktop\\swords\\websites\\websites\\marketing\\src\\custom\\global\\observer.tsx",["59","60","61"],"C:\\Users\\Chris Wegner\\Desktop\\swords\\websites\\websites\\marketing\\src\\components\\hero\\hero.tsx",[],"C:\\Users\\Chris Wegner\\Desktop\\swords\\websites\\websites\\marketing\\src\\components\\index.tsx",[],"C:\\Users\\Chris Wegner\\Desktop\\swords\\websites\\websites\\marketing\\src\\custom\\animation\\animater.class.tsx",["62"],"import React, {useContext, useRef} from 'react';\r\nimport styled from 'styled-components';\r\nimport {screen, SectionContext} from '../'\r\n\r\nexport class AnimaterOptions{\r\n  private _buffer = .2;\r\n  private _peak = .7;\r\n  repeat = false;\r\n\r\n  start ={\r\n    margin:{\r\n      top: 0,\r\n      right: 0,\r\n      bottom: 0,\r\n      left: 0\r\n    }\r\n  }\r\n\r\n  move ={\r\n    up: 0,\r\n    right: 0,\r\n    down: 0,\r\n    left: 0\r\n  }\r\n\r\n  set buffer(val:number){\r\n    this._buffer = val/100\r\n  }\r\n\r\n  get buffer():number{\r\n    return this._buffer;\r\n  }\r\n\r\n  set peak(val: number){\r\n    this._peak = val/100\r\n  }\r\n\r\n  get peak():number{\r\n    return this._peak;\r\n  }\r\n}\r\n\r\nexport class AnimaterControl{\r\n  private _options: AnimaterOptions;\r\n  private _pageYOffset = 0;\r\n  private _percentPageViewed = 0;\r\n  private _scrollingDown = true;\r\n  private _revealed = false;\r\n  private _browserHeight = 0;\r\n  private _elementYOffset = 0;\r\n\r\n  constructor(opts:AnimaterOptions, broHeight:number, eleOffset){\r\n    this._options = opts\r\n    this._browserHeight = broHeight;\r\n    this._elementYOffset = eleOffset;\r\n  }\r\n\r\n  updateAnimater(onScreen, offset){\r\n    if(offset>this._pageYOffset){\r\n      this._scrollingDown = true\r\n    }else{\r\n      this._scrollingDown = false\r\n    }\r\n    this._pageYOffset = offset;\r\n    this._percentPageViewed = onScreen;\r\n  }\r\n\r\n  set updatePosition(val){\r\n    this._percentPageViewed = val;\r\n  }\r\n\r\n  get appliedRatio(){\r\n    const screenPosition =  (this._elementYOffset-this._pageYOffset)/this._browserHeight\r\n    const rawRatio = (screenPosition + this._options.buffer)>1? 1 : (screenPosition + this._options.buffer);\r\n    const appliedPeak = this._scrollingDown?this._options.peak : 1-this._options.peak\r\n    const appliedRatio = 1 - Math.abs(rawRatio - this._options.peak)\r\n    return appliedRatio\r\n  }\r\n\r\n  get margins():string{\r\n    return `\r\n      ${this._options.start.margin.top + (this._options.move.down * this.appliedRatio)}px \r\n      ${this._options.start.margin.right + (this._options.move.left * this.appliedRatio)}px \r\n      ${this._options.start.margin.bottom + (this._options.move.up * this.appliedRatio)}px \r\n      ${this._options.start.margin.left + (this._options.move.right * this.appliedRatio)}px\r\n    `;\r\n  }\r\n}\r\n\r\ninterface IContainer{\r\n  attrs: AnimaterControl;\r\n  ref?:any;\r\n}\r\n\r\nconst Container = styled.div<IContainer> `\r\n  margin: 0;\r\n\r\n  @media(min-width: ${screen.md}){\r\n    margin: ${p=>p.attrs.margins};\r\n  }\r\n`;\r\n\r\nexport const Animater=({children, opts = new AnimaterOptions()})=>{\r\n  const ref = useRef(null)\r\n  const onScreen = useContext(SectionContext)\r\n\r\n  let animaterControl: AnimaterControl\r\n  if(ref.current){\r\n    animaterControl = new AnimaterControl(opts, window.innerHeight, ref.current.offsetTop);\r\n  }else{\r\n    animaterControl = new AnimaterControl(opts, window.innerHeight, 0);\r\n  }\r\n\r\n  animaterControl.updateAnimater(onScreen, window.pageYOffset)\r\n\r\n  return(\r\n    <Container ref={ref} attrs={animaterControl}>\r\n      {children}\r\n    </Container>\r\n  ) \r\n}","C:\\Users\\Chris Wegner\\Desktop\\swords\\websites\\websites\\marketing\\src\\custom\\animation\\animater.test.tsx",["63"],{"ruleId":"64","severity":1,"message":"65","line":4,"column":20,"nodeType":"66","messageId":"67","endLine":4,"endColumn":29},{"ruleId":"64","severity":1,"message":"68","line":1,"column":42,"nodeType":"66","messageId":"67","endLine":1,"endColumn":51},{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","severity":1,"message":"74","line":32,"column":15,"nodeType":"75","endLine":32,"endColumn":45},{"ruleId":"64","severity":1,"message":"76","line":2,"column":8,"nodeType":"66","messageId":"67","endLine":2,"endColumn":14},{"ruleId":"64","severity":1,"message":"77","line":1,"column":8,"nodeType":"66","messageId":"67","endLine":1,"endColumn":13},{"ruleId":"73","severity":1,"message":"78","line":42,"column":30,"nodeType":"66","endLine":42,"endColumn":37},{"ruleId":"73","severity":1,"message":"79","line":44,"column":6,"nodeType":"80","endLine":44,"endColumn":8,"suggestions":"81"},{"ruleId":"64","severity":1,"message":"82","line":75,"column":11,"nodeType":"66","messageId":"67","endLine":75,"endColumn":22},{"ruleId":"83","severity":